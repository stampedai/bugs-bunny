{
  "name": "Bugs Bunny",
  "description": "Browser extension for ops to report bugs easily",
  "version": "1.0.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.stamped.ai/*"
      ],
      "js": [
        "content-scripts.js"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "scripting",
    "webRequest",
    "identity",
    "identity.email",
    "webNavigation",
    "tabs"
  ],
  "action": {
    "default_popup": "popup/index.html",
    "default_title": "Bugs Bunny",
    "default_icon": {
      "16": "icon.png",
      "32": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}